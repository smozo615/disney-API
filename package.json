{
  "name": "disney-api",
  "version": "1.0.0",
  "description": "REST API to explore disney world",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "start:dev": "nodemon app.js",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "migrations:run": "sequelize-cli db:migrate",
    "migrations:status": "sequelize-cli db:migrate:status",
    "migrations:revert": "sequelize-cli db:migrate:undo",
    "migrations:delete": "sequelize-cli db:migrate:undo:all",
    "seeds:generate": "sequelize-cli seed:generate --name",
    "seeds:run": "sequelize-cli db:seed",
    "seeds:revert": "sequelize-cli db:seed:undo",
    "seeds:runAll": "sequelize-cli db:seed:all",
    "seeds:delete": "sequelize-cli db:seed:undo:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smozo615/disney-API.git"
  },
  "keywords": [
    "Node",
    "Express",
    "REST",
    "API",
    "Sequelize",
    "PostgreSQL"
  ],
  "author": "Santiago Mozo <smozo615@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/smozo615/disney-API#readme",
  "dependencies": {
    "@hapi/boom": "10.0.0",
    "bcryptjs": "2.4.3",
    "express": "4.18.1",
    "joi": "17.6.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "6.21.3",
    "sequelize-cli": "6.4.1",
    "swagger-jsdoc": "6.2.5",
    "swagger-ui-express": "4.5.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "eslint": "8.21.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "nodemon": "2.0.19",
    "prettier": "2.7.1"
  }
}
